@{
    ViewData["Title"] = "Search";
}

<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
      <div class="container">

        <ol>

          <li>Valuations</li>
        </ol>
        <h2>Search Property</h2>

      </div>
    </section><!-- End Breadcrumbs -->

    <!-- ======= Contact Section ======= -->
    <section id="contact" class="contact">
      <div class="container">


        <div class="row">

          <div class="col-lg-6">
            <form  asp-action="Index" method="post" id="form" class="php-email-form">
              <div class="form-group mt-3">
                <input type="text" class="form-control" value="@ViewData["PremiseID"]" placeholder="PremiseID" id="PremiseID" name="PremiseID">
              </div>
              <div class="form-group mt-3">
                <input type="text" class="form-control"value="@ViewData["Town"]" placeholder="Township" id="Township" name="Township">
              </div>
              <div class="form-group mt-3">
                <input type="text" class="form-control" value="@ViewData["ERF"]"  placeholder="ERF" id="StandNo" name="StandNo">
              </div>
           <div class="my-3">
                            @*<div class="loading">Loading</div>
                <div class="error-message"></div>
                <div class="sent-message">Your message has been sent. Thank you!</div>*@
              </div
              <div class="text-center"><button type="submit">Search</button></div>
            </form>
          </div>

        </div>

      </div>
    </section><!-- End Contact Section -->

  </main><!-- End #main -->
